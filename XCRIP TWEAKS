    <script>
        // --- L√≥gica del Fondo Topogr√°fico Animado (Canvas) ---
        const canvas = document.getElementById('topographic-bg');
        const ctx = canvas.getContext('2d');
        let width, height;
        let lines = [];
        const lineCount = 25; 
        
        // Color Blanco/Gris sutil para el esquema monocrom√°tico
        const lineColor = 'rgba(255, 255, 255, 0.2)'; 
        const speed = 0.00008; // Velocidad de movimiento sutil
        let timeOffset = Math.random() * 10000; 
        let animationFrameId;

        function resizeCanvas() {
            width = canvas.width = window.innerWidth;
            height = canvas.height = window.innerHeight;
            
            lines = [];
            for (let i = 0; i < lineCount; i++) {
                lines.push({
                    yOffset: height * (i / lineCount) * 1.5 - (height * 0.25), 
                    amplitude: Math.random() * 20 + 20, // Aumentado ligeramente para m√°s efecto
                    frequency: Math.random() * 0.005 + 0.008, // Aumentado ligeramente para m√°s onda
                    phase: Math.random() * 2 * Math.PI 
                });
            }
        }

        function drawTopography(time) {
            ctx.clearRect(0, 0, width, height);
            
            const currentTime = time * speed + timeOffset;

            lines.forEach((line, index) => {
                ctx.beginPath();
                ctx.strokeStyle = lineColor;
                ctx.lineWidth = 1 + (index / lineCount) * 0.5;
                
                const sweepShift = index * 0.02;

                for (let x = 0; x <= width; x += 30) {
                    const y = line.yOffset + 
                              Math.sin(x * line.frequency + currentTime + line.phase + sweepShift) * line.amplitude; 
                    
                    if (x === 0) {
                        ctx.moveTo(x, y);
                    } else {
                        ctx.lineTo(x, y);
                    }
                }
                
                const lastY = line.yOffset + 
                              Math.sin(width * line.frequency + currentTime + line.phase + sweepShift) * line.amplitude; 
                ctx.lineTo(width, lastY);

                ctx.stroke();
            });

            // Llamada recursiva para el bucle de animaci√≥n
            animationFrameId = requestAnimationFrame(drawTopography);
        }

        // Inicializa y comienza la animaci√≥n
        window.addEventListener('resize', resizeCanvas);
        window.onload = function () {
            resizeCanvas();
            // Iniciar la animaci√≥n en la carga de la ventana
            drawTopography(performance.now()); // Usar performance.now() para un tiempo m√°s preciso
            
            // Forzar la animaci√≥n de entrada para todos los elementos
            document.querySelectorAll('.animated-item').forEach(el => {
                el.style.opacity = '1';
                el.style.transform = 'translateY(0)';
            });
        };
        
        window.onbeforeunload = () => {
            cancelAnimationFrame(animationFrameId);
        };
        
    </script>
</body>
</html>
